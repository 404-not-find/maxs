<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.projectmaxs.module.smsread"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permisson android:name="projectmaxs.permission.USE_MAIN"/>
    <application android:label="@string/app_name" >
        <activity android:name=".panels.MainActivitySmsRead"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="org.projectmaxs.module.smsread.RegisterWithMainService">
            <intent-filter>
                <action android:name="org.projectmaxs.REGISTER_MODULE"/>
            </intent-filter>
        </service>
    </application>
</manifest> 
